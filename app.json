{
  "expo": {
    "name": "FocusOne",
    "slug": "FocusOne",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": [
      "wxdc022c6a39cb32b7",
      "focusone"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "weixin",
          "weixinULAPI",
          "weixinURLParamsAPI"
        ],
        "NSPhotoLibraryUsageDescription": "此应用需要访问相册来保存截图",
        "NSPhotoLibraryAddUsageDescription": "此应用需要访问相册来保存截图"
      },
      "associatedDomains": [
        "applinks:(focusone.ruidoc.cn)"
      ],
      "bundleIdentifier": "com.focusone"
    },
    "android": {
      "package": "com.focusone",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D0D12"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-native-wechat",
      "expo-iap"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ba49731-4a04-43da-adb9-e1e4311eebae"
      }
    },
    "owner": "freeshore"
  }
}