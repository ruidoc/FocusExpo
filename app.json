{
  "expo": {
    "name": "FocusOne",
    "slug": "FocusOne",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": [
      "wxdc022c6a39cb32b7",
      "focusone"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "weixin",
          "weixinULAPI",
          "weixinURLParamsAPI"
        ]
      },
      "associatedDomains": [
        "applinks:(focusone.com)"
      ],
      "bundleIdentifier": "com.focusone"
    },
    "android": {
      "package": "com.focusone",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#151718"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-native-wechat"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}